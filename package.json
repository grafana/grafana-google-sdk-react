{
  "name": "@grafana/google-sdk",
  "version": "0.3.5",
  "description": "Common Google features for grafana",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && rollup -c rollup.config.ts --configPlugin esbuild",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix",
    "lint:package": "publint",
    "verify-types": "attw --pack .",
    "typecheck": "tsc",
    "clean": "rm -rf ./dist ./compiled",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,md,mdx,yml,yaml,json,scss,css}\""
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grafana/grafana-google-sdk-react.git"
  },
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@grafana/data": ">=10.4.0",
    "@grafana/ui": ">=10.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@grafana/data": "^11.5.3",
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/tsconfig": "^2.0.0",
    "@grafana/ui": "^11.5.3",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@stylistic/eslint-plugin-ts": "^3.1.0",
    "@swc/core": "^1.11.24",
    "@swc/jest": "^0.2.38",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/identity-obj-proxy": "^3",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "cspell": "^9.0.1",
    "esbuild": "^0.25.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lodash": "^4.17.21",
    "publint": "^0.3.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "^4.40.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-node-externals": "^8.0.0",
    "tslib": "^2.3.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@4.12.0"
}
