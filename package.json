{
  "name": "@grafana/google-sdk",
  "version": "0.4.1",
  "description": "Common Google features for grafana",
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json && rollup -c rollup.config.ts --configPlugin esbuild",
    "lint": "eslint --cache .",
    "lint:fix": "yarn run lint --fix",
    "lint:package": "publint",
    "verify-types": "attw --pack .",
    "typecheck": "tsc",
    "clean": "rm -rf ./dist ./compiled",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,md,mdx,yml,yaml,json,scss,css}\""
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grafana/grafana-google-sdk-react.git"
  },
  "author": "Grafana Labs <team@grafana.com> (https://grafana.com)",
  "license": "Apache-2.0",
  "peerDependencies": {
    "@grafana/data": ">=11.6.0",
    "@grafana/ui": ">=11.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@grafana/data": "12.4.0",
    "@grafana/eslint-config": "9.0.0",
    "@grafana/tsconfig": "2.0.1",
    "@grafana/ui": "12.4.0",
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@stylistic/eslint-plugin-ts": "4.4.1",
    "@swc/core": "1.15.13",
    "@swc/jest": "0.2.39",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/identity-obj-proxy": "3.0.2",
    "@types/jest": "30.0.0",
    "@types/lodash": "4.17.24",
    "@types/react": "18.3.28",
    "@typescript-eslint/eslint-plugin": "8.56.1",
    "@typescript-eslint/parser": "8.56.1",
    "cspell": "9.7.0",
    "esbuild": "0.27.3",
    "eslint": "10.0.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jsdoc": "62.7.1",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "lodash": "4.17.23",
    "prettier": "3.8.1",
    "publint": "0.3.17",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rollup": "4.59.0",
    "rollup-plugin-dts": "6.3.0",
    "rollup-plugin-esbuild": "6.2.1",
    "rollup-plugin-node-externals": "8.1.2",
    "tslib": "2.8.1",
    "typescript": "5.9.3"
  },
  "packageManager": "yarn@4.12.0"
}
